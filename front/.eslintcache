})\n        let playlist = playlists.find(item => item.name === this.state.uri);\n        if (playlist) {\n            this.setState({\n                playlist: {\n                    id: playlist.id,\n                    name: playlist.name,\n                    userId: playlist.userId,\n                    public: playlist.public,\n                    musicsId: playlist.musicsId,\n                    image_path: playlist.image_path,\n                }\n            });\n            this.getMusics(playlist);\n        } else\n            this.props.history.push('/');\n    }\n\n    getMusics(playlist) {\n        let musics = [];\n        playlist.musicsId.forEach(id => {\n            musics.push(MusicService.getMusics().find(music => music._id === id));\n        });\n        this.setState({musics: musics});\n    }\n\n    render() {\n        return (\n            <div className=\"global-container\">\n                <div className=\"row\">\n                    <div className=\"col-3\">\n                        {\n                            this.state.playlist.image_path ?\n                                <Image src={`images/${this.state.playlist.image_path}`} className=\"playlist-default playlist-cover\"\n                                       alt=\"Playlist image\"/>\n                                :\n                                <Image src='images/playlist-default.png' className=\"playlist-default playlist-cover\"\n                                       alt=\"Playlist image\"/>\n                        }\n                    </div>\n                    <div className=\"col-7 playlist-test\">\n                        <h5>Concocté par l'utilisateur #{this.state.playlist.userId}</h5>\n                        <h3>{this.state.playlist.name}</h3>\n                        <small>Playlist {this.state.playlist.public ? 'publique' : 'privée'}, {this.state.playlist.musicsId.length} titres\n                            ━ 4H30</small>\n\n                        <div className=\"header-buttons\">\n                            { this.state.playlist.musicsId.length > 0 && <Button>Lire</Button> }\n                            <Button>Enregistrer</Button>\n                        </div>\n                    </div>\n                </div>\n\n                <div className=\"row\">\n                    <div className=\"playlist-container\">\n                        <Table className=\"music-table\" hover>\n                            <thead>\n                            <tr>\n                                <th className=\"table-header\"/>\n                                <th className=\"table-header\">Titre</th>\n                                <th className=\"table-header\">Artiste</th>\n                                <th className=\"table-header\">Album</th>\n                                <th className=\"table-header\">Durée</th>\n                            </tr>\n                            </thead>\n                            <tbody>\n                            {\n                                this.state.musics.length > 0 ?\n                                    this.state.musics.map(function (music, i) {\n                                        return <tr className=\"music-tr\">\n                                            <td><HiHeart/></td>\n                                            <td>{music.name}</td>\n                                            <td>{music.artiste}</td>\n                                            <td>{music.album}</td>\n                                            <td>{music.time}min</td>\n                                        </tr>\n                                    })\n                                    :\n                                    <tr className=\"music-tr\">\n                                        <td colSpan=\"5\">Aucune musique à afficher pour le moment</td>\n                                    </tr>\n                            }\n                            </tbody>\n                        </Table>\n                    </div>\n                </div>\n            </div>\n        );\n    }\n}","/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/musics/addMusic.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/playlists/addPlaylist.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/musics/updateMusic.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/administration.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/categories/addCategory.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/categories/updateCategory.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/playlists/updatePlaylist.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/components/menu/menu.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/musics/musics.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/playlists/playlists.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/categories/categories.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/components/playlist-card/playlist-card.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/edit-profile/edit-profile.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/services/playlist.service.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/services/music.service.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/services/category.service.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/topbar/topbar.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/administration/users/users.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/login/login.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/services/user.service.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/views/register/register.js",[],"/home/baptiste/H3 HITEMA/yfitops/front/src/rootReducer.js",[],"/home/baptiste/H3 HITEMA/yfitops/f